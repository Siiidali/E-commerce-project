{
  "compilerOptions": {
    "target": "es2016",
    "outDir": "./build",
    "module": "commonjs",
    "types": ["node", "multer"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true
  }
}
